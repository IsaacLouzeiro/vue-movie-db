n  setup() {\n    const movie = ref({});\n    const route = useRoute();\n\n    onBeforeMount(() => {\n      fetch(`http://www.omdbapi.com/?apikey=${env.apikey}&i=${route.params.id}&plot=full`).then(response => response.json()).then(data => {\n        movie.value = data;\n        console.log(data)\n      })\n    });\n\n    return {\n      movie\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.movie-detail {\n  padding: 16px;\n  \n  h2 {\n    color: #fff;\n    font-size: 1.5em;\n    font-weight: 600;\n    margin-bottom: 16px;\n  }\n\n  .featured-img {\n    display: block;\n    max-width: 200px;\n    margin-bottom: 10px;\n  }\n  \n  p {\n    color: #fff;\n    font-size: 1em;\n    line-height: 1.4;\n    margin: 3px 0;\n\n    span {\n      color: #42b883;\n    }\n  }\n\n  .plot-movie {\n  }\n\n  .type-movie {\n    display: inline-block;\n    padding: 5px 16px;\n    background-color: #42b883;\n    margin: 10px 0;\n  }\n}\n</style>"],"sourceRoot":""}]}